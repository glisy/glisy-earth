{
  "name": "glisy-earth",
  "version": "0.0.2",
  "repo": "glisy/glisy-earth",
  "description": "Glisy example rendering glsl-earth and using glsl-fog for fade in effects.",
  "keywords": [
    "glsl",
    "stack.gl",
    "earth"
  ],
  "global": true,
  "install": "make && cp -f program ${PREFIX:-/usr/local}/bin/glisy-earth",
  "dependencies": {
    "glisy/glfw-shell": "0.0.5",
    "glisy/geometry": "0.0.2",
    "glisy/uniform": "0.0.1",
    "glisy/program": "0.0.4",
    "glisy/shader": "0.0.1",
    "glisy/color": "0.0.5",
    "glisy/math": "0.0.4",
    "glisy/gl": "0.0.1",
    "jwerle/fs.c": "0.1.2"
  }
}
